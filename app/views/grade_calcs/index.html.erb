<h1>Listing grade_calcs</h1>

<table>
  <thead>
    <tr>
      <th>Gradecalc</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @grade_calcs.each do |grade_calc| %>
      <tr>
        <td><%= grade_calc.gradecalc %></td>
        <td><%= link_to 'Show', grade_calc %></td>
        <td><%= link_to 'Edit', edit_grade_calc_path(grade_calc) %></td>
        <td><%= link_to 'Destroy', grade_calc, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Grade calc', new_grade_calc_path %>
